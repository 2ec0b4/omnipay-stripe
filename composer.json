{
    "name": "omnipay/stripe",
    "type": "library",
    "description": "Stripe driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "stripe"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-stripe",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-stripe/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Stripe\\" : "src/" }
    },
    "require": {
	"php": "^7",
        "omnipay/common": "dev-master"
    },
    "require-dev": {
        "omnipay/tests": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
